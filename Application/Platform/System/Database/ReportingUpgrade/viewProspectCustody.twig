FROM PeoplePerson_{{ ConsumerAcronym }}.tblPerson tP
    JOIN SettingConsumer_{{ ConsumerAcronym }}.viewGroup vG ON vG.TblPerson_Id = tP.Id
WHERE vG.TblGroup_MetaTable = 'PROSPECT'
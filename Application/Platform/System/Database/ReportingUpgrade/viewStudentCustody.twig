    ) TblMail_AddressCompany_S2,

    -- Person S3
    -- Definition S3 Id
    (select vRTP.TblToPerson_serviceTblPersonTo
        from SettingConsumer_{{ ConsumerAcronym }}.viewRelationshipToPerson vRTP
        where vP.TblPerson_Id = vRTP.TblToPerson_serviceTblPersonFrom

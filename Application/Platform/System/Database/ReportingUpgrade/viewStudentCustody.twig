from SettingConsumer_{{ ConsumerAcronym }}.viewPerson vP
    JOIN SettingConsumer_{{ ConsumerAcronym }}.viewGroup vG ON vG.TblPerson_Id = vP.TblPerson_Id
where vG.TblGroup_MetaTable = 'STUDENT'